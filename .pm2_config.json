{
  "apps" : [{
    "name"        : "gf-skeleton-node",
    "script"      : "./entry.js",
    "watch"       : false,
    "exec_interpreter" : "node",
    "exec_mode"        : "fork",
    "instances" :     4,
    "error_file"        : "logs/err.log",
    "out_file"        : "logs/out.log",
    "merge_logs"      : true,
    "log_date_format" : "YYYY-MM-DD HH:mm Z",
    "env": {
      "PORT": "9321",
      "DEBUG": "Ctrl*",
      "NODE_ENV": "product"
    }
  }]
}

